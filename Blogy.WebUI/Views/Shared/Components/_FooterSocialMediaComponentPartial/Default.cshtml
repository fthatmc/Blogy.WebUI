@model List<SocialMedia>
<div class="col-lg-4">
    <div class="widget">
        <h3 class="mb-4">Hakkımızda</h3>
        <p>Birbirinde yetenekli blogger ve yazarlarımızla güncel ve ilgi çekici konularda içerik üretmeye devam ediyoruz.. <a href="#"></a></p>
    </div> <!-- /.widget -->
    <div class="widget">
        <h3>Sosyal Medya Hesaplarımız</h3>
        <ul class="list-unstyled social">
            @foreach(var item in Model)
            {
                <li><a href="@item.Url"><span class="@item.Icon"></span></a></li>
            }
            
        </ul>
    </div> <!-- /.widget -->
</div>
